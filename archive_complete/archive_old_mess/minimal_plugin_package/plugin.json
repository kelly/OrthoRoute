{
  "$schema": "https://go.kicad.org/api/schemas/v1/plugin.json",
  "identifier": "com.github.minimal.test",
  "name": "Minimal Track Test",
  "description": "Minimal IPC plugin that draws exactly one test track",
  "version": "1.0.0",
  "author": {
    "name": "test",
    "contact": "https://github.com/bbenchoff/OrthoRoute"
  },
  "license": "WTFPL",
  "requirements": {
    "python": ">=3.8",
    "packages": [
      "kicad-python>=0.4.0"
    ]
  },
  "runtime": {
    "type": "python",
    "python_interpreter": "auto"
  },
  "actions": [
    {
      "identifier": "minimal.draw_track",
      "name": "Draw One Track",
      "description": "Draw exactly one test track using IPC API",
      "category": "Testing",
      "show_in_toolbar": true,
      "entrypoint": "minimal_track_plugin.py"
    }
  ]
}
